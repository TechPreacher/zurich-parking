    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: List",
            "type": "debugpy",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "zurich_parking",
            "cwd": "${workspaceFolder}/src",
            "args" : ["--list"]
        },
        {
            "name": "Python Debugger: Show",
            "type": "debugpy",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "zurich_parking",
            "cwd": "${workspaceFolder}/src",
            "args" : ["--show", "Urania"]
        }
    ]
}